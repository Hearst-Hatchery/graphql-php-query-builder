# editorconfig.org
# Sublime plugin can be installed via Package Control
# search for editorconfig
root = true

[*]
indent_style = space
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.php]
indent_style = space
indent_size = 4

[composer.json]
indent_style = space
indent_size = 4
